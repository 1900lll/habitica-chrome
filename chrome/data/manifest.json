{

   "name": "Habitica",
   "version": "2.5.2",
   "permissions": [
      "storage", "tabs", "webNavigation", "notifications",
      "https://habitrpg.com/", "https://habitica.com/", "http://tomato.es/", "https://app.asana.com/",
      "https://mail.google.com/", "https://workflowy.com/", "https://pomodoro-tracker.com/"
    ],

   "description": "Monitors your browsing habits to divvy points to your Habitica character.",
   "web_accessible_resources": [ "img/icon-48.png", "img/icon-48-up.png", "img/icon-48-down.png", "img/icon-48-alert.png", "img/icon-48-inactive.png" ],
   "icons": {
      "128": "img/icon-128.png",
      "19": "img/icon-19.png",
      "48": "img/icon-48.png"
   },

   "manifest_version": 2,

   "options_page": "options.html",
   "background": {
        "page": "background.html"
   },

   "browser_action": {
      "default_icon": "img/icon-48-alert.png",
      "default_title": "Habitica",
      "default_popup": "popup.html"
    },
  "content_scripts": [
    {
      "matches": [
        "https://pomodoro-tracker.com/"
      ],
      "js": [
        "vendor/jquery-2.2.0.min.js",
        "vendor/jquery.onchange.plugin.js",
        "bind_pomodoroTracker.js"
      ]
    },
    {
      "matches": [
        "*://*.tomato.es/"
      ],
      "js": [
        "vendor/jquery-2.2.0.min.js",
        "bind_tomatoes.js"
      ]
    },
    {
      "matches": [
        "https://app.asana.com/*"
      ],
      "js": [
        "bind_asana.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://workflowy.com/*"
      ],
      "js": [
        "bind_workflowy.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "bind_googletask_gmail.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.google.com/calendar/*"
      ],
      "js": [
        "bind_googletask_calendar.js"
      ],
      "run_at": "document_end"
    }
  ],

   "key": "MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQDqmcPXz4CjBphP+HlVjJXbzLI3RrOlEtXJkdd5vVs57nm/uV5wC61SjMh5jDNRfQ/cJ6C/E10k2qp3OKv0vrY1f4HK2MNJ6WTUF1byqk3Rrpj4V0DlKS69ZqzHr4D1w2WfdHt/W43k+1G53ZKsXrmgxaFHsX7O2wg55FUb8Yr7pQIDAQAB"
}
