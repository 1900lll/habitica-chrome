{
  "manifest_version": 2,
  "name": "HabitRPG Chrome Extension",
  "version": "0.0.1",
  "description": "Plugs into various websites to give you Exp for positive tasks.",
  "options_page": "options.html",
  "content_scripts": [
  	{
      "matches": [
        "http://workflowy.com/*","https://workflowy.com/*",
        "http://9gag.com/*",
        "http://www.reddit.com/*", 
        "http://www.facebook.com/*"
       ],
      "js": [
        "jquery.js", 
        "underscore.js", 
        "habitrpg-options.js", 
        "habitrpg-workflowy.js"
       ],
      "runat":"document_end"
    }
  ]
}
